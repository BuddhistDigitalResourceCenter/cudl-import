                    <msContents defective="true">
                        <summary>This small-size black-paper manuscript containing the <title xml:lang="san">Mahāsaṃvarahṛdayadhāraṇī</title> has the peculiarity of being written in inks of three different colours: gold, bronze and silver.</summary>
                        
                        <msItem n="1">

                            <author>
                                <name type="person" role="aut" key="local_00000">
                           <persName xml:lang="eng" type="standard">‏Unknown</persName>
                        </name>
                            </author>
                        
                            <title xml:lang="san">Mahāsaṃvarahṛdayadhāraṇī</title>
                            
                            <incipit xml:lang="san">
                        <locus from="3r" to="3r">[3r1]</locus> namo viśvanāthāya || oṃ namaḥ sarvvabodhisatvāya devībhyo namaḥ buddhaḍākinībhyo namaḥ</incipit>
                            
                            <explicit xml:lang="san">
                        <locus from="10v" to="10v">[10v3]</locus> evaṃ rupan tu mahāvajra,herukaṃ tu vibhāvaye ṃ| [10v4] 
                                sarvvasatvāsamāyuktaṃ,mokṣasiddhiñ ca dehi<unclear>ne</unclear> 
                        <unclear>|</unclear> oṃ āḥ kāyavāṅcittavajra hraṃ phaṭ_ || [10v5]
                                oṃ vajravārahīmahādevī hraṃ phaṭ_ || 
                                <!--CHECK-->
                            </explicit>
                            
                            <finalRubric xml:lang="san">
                        <locus from="3r" to="3r">[3r4]</locus> iti mahāsamvarahṛdaya samāptaḥ || <g>@</g> || </finalRubric>

                            <textLang mainLang="san">Sanskrit.</textLang>
                        </msItem>
                    </msContents>
                    
