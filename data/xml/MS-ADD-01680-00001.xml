                    <msContents n="1">

                        <summary> A Nepalese palm-leaf manuscript of the <title xml:lang="san">Bhadracarīpraṇidhānarājā</title>, dated 1068 CE, used by Watanabe
                            for his edition (manuscript C). It contains the northern recension of
                            the text (Watanabe 1912, 13-15). </summary>

                        <msItem n="1" defective="false">

                            <author>
                                <name type="person" role="aut" key="local_00000">
                                    <persName xml:lang="eng" type="standard">‏Unknown.</persName>
                                </name>
                            </author>

                            <title xml:lang="san">Bhadracarīpraṇidhāna</title>
                            <title xml:lang="san" type="alt">Bhadracarīpraṇidhānarājā</title>
                            <title xml:lang="san" type="alt">Bhadracarīnāmāryasamantabhadrapraṇidhāna</title>
                            <title xml:lang="san" type="alt">Āryabhadracarīpraṇidhānarājā</title>
                            <title xml:lang="san" type="alt">Āryabhadracarīmahāpraṇidhānarājā</title>
                            <title xml:lang="san" type="alt">Āryasamantabhadracaryāpraṇidhānarājā</title>
                            <title xml:lang="san" type="alt">Bhadracarīpraṇidhānagāthā</title>
                            <title xml:lang="san" type="alt">Bhadracaryāgāthā</title>


                            <rubric xml:lang="san">
                        <locus from="1v" to="1v">[1v1]</locus>
                                <damage>-3-</damage>
                        <damage>samantabhadrāya</damage> || </rubric>

                            <incipit xml:lang="san">
                        <locus from="1v" to="1v">[1v1]</locus> atha
                                khalu samantabhadro bodhi¦<g>%</g>sattvo mahāsattvo <choice>
                                    <orig>'nekalokadhātuparamparābhilāpyānabhilāpyabuddhakṣetraparamāṇu<damage>-2-</damage>[1v2]<damage>-1-</damage>mān_</orig>
                                    <reg type="hyphenated">'neka-lokadhātu-paramparābhilāpyānabhilāpya-buddhakṣetra-paramāṇu<damage>-2-</damage>[1v2]<damage>-1-</damage>mān_</reg>
                                </choice> kalpān_ kalpaprasarāṃś cābhidyotayamāno bhūya¦<g>%</g>syā
                                mātrayā gāthābhir gītena praṇidhānam akārṣit_ || <g>@</g> || </incipit>

                            <explicit xml:lang="san">
                        <locus from="6r" to="6r">[6r5]</locus>
                                    bhadracarīpraṇidhānapaṭhi<g>%</g>tvā, yat kuśalam mayi sañcitu
                                kiñcit <add place="inline">||</add> ekaṣaṇena samṛddhyatu sarvaṃ
                                tena jagasya <sic>subham</sic> prāṇi[6r6]dhānaṃ || </explicit>

                            <finalRubric xml:lang="san">
                        <locus from="6r" to="6r">[6r6]</locus>
                                āryabhadracarīmahāpraṇidhānarājaṃ samā¦<g>%</g>ptaṃ || <g>@</g> || </finalRubric>

                            <colophon xml:lang="san">
                        <locus from="6r" to="6r">[6r6]</locus> ye
                                dharmā hetuprabhavā hetun teṣāṇ tathāgato hy avadat teṣāñ ca yo
                                nirodha evamvadī [6r7] <damage>-1-</damage>
                                <damage>mahāśramaṇaḥ</damage> || <g>@</g> || samvat_ 188
                                    bhadrapadaśu<g>%</g>kla paurṇṇamāsyāḥ śukradine |
                                    <damage>upāsakacinaiśiṣyāpakasya</damage> pustako yaṃ likhitam
                                iti^^^|| </colophon>

